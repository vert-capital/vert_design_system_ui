#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/debora/www/vert_design_system_ui/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/debora/www/vert_design_system_ui/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../ts-node@10.9.1_kluoused5zacjtflizwvdqgpom/node_modules/ts-node/dist/bin-transpile.js" "$@"
else
  exec node  "$basedir/../../../../../ts-node@10.9.1_kluoused5zacjtflizwvdqgpom/node_modules/ts-node/dist/bin-transpile.js" "$@"
fi
